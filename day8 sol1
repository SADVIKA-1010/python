a=int(input('enter a value'))
b=int(input('enter a value'))
c=int(input('enter a value'))
d=int(input('enter a value'))
if a>=b:
    if a>=c:
        if a>=d:
            print(a,"is big")
if b>=a:
    if b>=c:
        if b>=d:
            print(b,"is big")
if c>=a:
    if c>=b:
        if c>=d:
            print(c,"is big")
if d>=a:
    if d>=d:
        if d>=c:
            print(d,"is big")
